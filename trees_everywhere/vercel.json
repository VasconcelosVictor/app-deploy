{
  "version": 2,
  "builds": [
    {
      "src": "trees_everywhere/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "trees_everywhere/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "trees_everywhere/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "trees_everywhere/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "DATABASE_URL": "postgres://postgres:postgres@localhost:5432/testando",
      "SECRET_KEY": "django-insecure-d28kgj1jace&65w_0z01+g+lq1z$sy^@93t*z1#0hjbots)bvm",
      "DEBUG": "False"
    },
    "commands": {
      "build": "pip install -r requirements.txt && python3 manage.py migrate"
    }
  }
}
